  "scripts": {
    "start": "env-cmd -f ./.env.local webpack-dev-server --color --hot --mode=development --disable-host-check",
    "start:test": "env-cmd -f ./.env.test webpack-dev-server --color --hot --mode=development --disable-host-check",
    "build:prod": "env-cmd -f ./.env.prod webpack --mode=production",
    "build:dev": "env-cmd -f ./.env.dev webpack --mode=development",
    "build:local": "env-cmd -f ./.env.local webpack --mode=development",
    "lint": "eslint .",
    "test": "./scripts/test.sh",
    "test:ci": "cypress run",
    "report": "cat ./coverage/lcov.info | env-cmd -f ./.env.test codacy-coverage",
